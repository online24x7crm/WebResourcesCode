function WorkOrderFieldlock(){
	var status = Xrm.Page.getAttribute("onl_spectra_orderstatus").getValue();
	
	if(status>=122050002)
		{ 
			Xrm.Page.getControl("onl_spectra_cpeserialnumber").setDisabled(true);  
			Xrm.Page.getControl("onl_spectra_cpetrackingnumber").setDisabled(true);
			Xrm.Page.getControl("onl_spectra_dispatcheddate").setDisabled(true);
        }
	else if(status>122050000 || status>122050001 )
		{ 
			Xrm.Page.getControl("onl_spectra_cpeserialnumber").setDisabled(false);  
			Xrm.Page.getControl("onl_spectra_cpetrackingnumber").setDisabled(false);
			Xrm.Page.getControl("onl_spectra_dispatcheddate").setDisabled(false);
        }
}
