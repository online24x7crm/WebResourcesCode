function lockPresales(){
debugger;

var status = Xrm.Page.getAttribute("onl_status").getValue();

if(status==122050003 || status==122050001){
//Xrm.Page.getControl("spectra_salutation").setDisabled(true);  
 Xrm.Page.getControl("spectra_contactpersonname").setDisabled(true);
