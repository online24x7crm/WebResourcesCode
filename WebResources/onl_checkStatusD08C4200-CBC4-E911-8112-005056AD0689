Xrm.Page.getControl("onl_iscustomerusinganyroutingservices").setDisabled(true);
Xrm.Page.getControl("onl_whenisthefirewallutmlicensesettoexpire").setDisabled(true);
Xrm.Page.getControl("onl_isthereexistingmplsbackbone").setDisabled(true);
Xrm.Page.getControl("onl_whenisthemplscontracttoberenewed").setDisabled(true);
Xrm.Page.getControl("onl_indicationofitspent").setDisabled(true);
Xrm.Page.getControl("onl_currentoperationalcities").setDisabled(true);
Xrm.Page.getControl("onl_plsmentionnumberoflinks").setDisabled(true);
Xrm.Page.getControl("onl_networksecurityservicesusedbycustomeronl").setDisabled(true);
