Xrm.Page.getControl("onl_iscustomerusinganyroutingservices").setDisabled(false);
Xrm.Page.getControl("onl_whenisthefirewallutmlicensesettoexpire").setDisabled(false);
Xrm.Page.getControl("onl_isthereexistingmplsbackbone").setDisabled(false);
Xrm.Page.getControl("onl_whenisthemplscontracttoberenewed").setDisabled(false);
Xrm.Page.getControl("onl_indicationofitspent").setDisabled(false);
Xrm.Page.getControl("onl_currentoperationalcities").setDisabled(false);
